{
    "name": "relia-prompt",
    "version": "1.0.0",
    "description": "Tool for testing llm prompts against multiple LLMs and improving them.",
    "main": "dist/server.js",
    "scripts": {
        "build": "bun run tsc",
        "start": "bun dist/server.js",
        "dev": "bun --watch src/server.ts",
        "format": "bun run prettier --write \"src/**/*.ts\" \"public/**/*.{html,js,css}\" \"*.json\" \"drizzle.config.ts\"",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "db:generate": "bun run drizzle-kit generate",
        "db:studio": "bun run drizzle-kit studio"
    },
    "dependencies": {
        "@aws-sdk/client-bedrock-runtime": "^3.940.0",
        "cors": "^2.8.5",
        "drizzle-orm": "^0.44.7",
        "express": "^5.1.0",
        "fast-deep-equal": "^3.1.3",
        "openai": "^6.9.1"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/bun": "^1.3.3",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/node": "^24.10.1",
        "better-sqlite3": "^12.4.6",
        "drizzle-kit": "^0.31.7",
        "eslint": "^9.39.1",
        "prettier": "^3.6.2",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.48.0"
    }
}
